{"ast":null,"code":"import emailjs from '@emailjs/browser';\nexport default {\n  name: \"ContactVue\",\n  created() {\n    document.title = \"Contact | Lily Dosedel\";\n  },\n  data() {\n    return {\n      from_name: \"\",\n      from_email: \"\",\n      message: \"\"\n    };\n  },\n  methods: {\n    sendEmail(e) {\n      try {\n        emailjs.sendForm(\"service_7yb38ys\", \"template_vtjrpr9\", e.target, \"Ko8zdPaMimYBcfLTm\", {\n          to_name: \"Lily\",\n          from_name: this.from_name,\n          from_email: this.from_email,\n          message: this.message\n        });\n        this.from_name = \"\";\n        this.from_email = \"\";\n        this.message = \"\";\n        this.togglePopup();\n      } catch (error) {\n        console.log({\n          error\n        });\n      }\n    },\n    togglePopup() {\n      document.getElementById(\"popup\").style.opacity = 1;\n      setTimeout(() => {\n        this.closePopup();\n      }, 2000);\n    },\n    closePopup() {\n      document.getElementById(\"popup\").style.opacity = 0;\n    }\n  }\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,OAAM,MAAO,kBAAkB;AAEtC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,OAAO,GAAG;IACNC,QAAQ,CAACC,KAAK,GAAC,wBAAuB;EAC1C,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACTC,SAAS,CAACC,CAAC,EAAE;MACT,IAAI;QACAX,OAAO,CAACY,QAAQ,CAAC,iBAAiB,EAAC,kBAAkB,EAAED,CAAC,CAACE,MAAM,EAC/D,mBAAmB,EAAE;UACjBC,OAAO,EAAE,MAAM;UACfR,SAAS,EAAE,IAAI,CAACA,SAAS;UACzBC,UAAU,EAAE,IAAI,CAACA,UAAU;UAC3BC,OAAO,EAAE,IAAI,CAACA;QAClB,CAAC;QACD,IAAI,CAACF,SAAQ,GAAI,EAAE;QACnB,IAAI,CAACC,UAAS,GAAI,EAAE;QACpB,IAAI,CAACC,OAAM,GAAI,EAAE;QACjB,IAAI,CAACO,WAAW,EAAE;MACtB,EAAE,OAAMC,KAAK,EAAE;QACXC,OAAO,CAACC,GAAG,CAAC;UAACF;QAAK,CAAC,CAAC;MACxB;IACJ,CAAC;IACDD,WAAW,GAAG;MACVZ,QAAQ,CAACgB,cAAc,CAAC,OAAO,CAAC,CAACC,KAAK,CAACC,OAAO,GAAC,CAAC;MAChDC,UAAU,CAAC,MAAM;QACb,IAAI,CAACC,UAAU,EAAC;MACpB,CAAC,EAAE,IAAI;IACX,CAAC;IACDA,UAAU,GAAG;MACTpB,QAAQ,CAACgB,cA